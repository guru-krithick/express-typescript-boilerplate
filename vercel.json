{
  "version": 2,
  "builds": [
    {
      "src": "src/vercel.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/docs/(.*)",
      "dest": "/src/vercel.ts"
    },
    {
      "src": "/api/docs/(.*)",
      "dest": "/src/vercel.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/src/vercel.ts"
    }
  ]
}