{
    "version": 2,
    "builds": [
      {
        "src": "dist/api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api-docs(.*)",
        "dest": "dist/api/index.js"
      },
      {
        "src": "/api/(.*)",
        "dest": "dist/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "dist/api/index.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }